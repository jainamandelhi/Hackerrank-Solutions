#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int n,arr[100];
    cin>>n;
    vector<int>ar(n);
    for(int i=0;i<n;i++)
    {
        cin>>ar[i];
    }
     for(int i=0;i<100;i++)
    {
        arr[i]=0;
    }
    for(int i=0;i<n;i++)
    {
        arr[ar[i]]++;
    }
    for(int i=0;i<100;i++)
    {
        cout<<arr[i]<<" ";
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}
