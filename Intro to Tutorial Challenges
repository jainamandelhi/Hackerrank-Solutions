#include<bits/stdc++.h>
using namespace std;


int main() {
    int n,i;
    cin>>i;
    cin>>n;
    vector<int>arr(n);
    for(int j=0;j<n;j++)
    {
        cin>>arr[j];
    }
    for(int j=0;j<n;j++)
    {
        if(arr[j]==i)
        {
            cout<<j;
            exit(0);
        }
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}
