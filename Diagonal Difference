#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main(){
    int n,sum=0,sum1=0;
    cin >> n;
    vector< vector<int> > a(n,vector<int>(n));
    for(int a_i = 0;a_i < n;a_i++){
       for(int a_j = 0;a_j < n;a_j++){
          cin >> a[a_i][a_j];
       }
    }
    for(int a_i = 0;a_i < n;a_i++){
       int a_j=a_i;
         {
             sum=sum+a[a_i][a_j];
         }
    }
     for(int a_i = 0;a_i < n;a_i++){
       int a_j=n-1-a_i;
         {
             sum1=sum1+a[a_i][a_j];
         }
     }
    cout<<abs(sum-sum1);
    return 0;
}
