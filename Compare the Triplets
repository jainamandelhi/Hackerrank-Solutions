#include <bits/stdc++.h>

using namespace std;

void solve(int a0, int a1, int a2, int b0, int b1, int b2){
   vector < int > count(2); count[0]=0; count[1]=0;
    if(a0>b0)
        count[0]++;
     if(a1>b1)
        count[0]++;
     if(a2>b2)
        count[0]++;
     if(a0<b0)
        count[1]++;
     if(a1<b1)
        count[1]++;
    if(a2<b2)
    {
        count[1]++;
    }
    
    cout<<count[0]<<" "<<count[1];
    // Complete this function
}

int main() {
    int a0;
    int a1;
    int a2;
    cin >> a0 >> a1 >> a2;
    int b0;
    int b1;
    int b2;
    cin >> b0 >> b1 >> b2;
     solve(a0, a1, a2, b0, b1, b2);
   
    

    return 0;
}
