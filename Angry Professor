#include <bits/stdc++.h>

using namespace std;

int main()
{
    int n,sum=0,p,q;
    cin>>n;
    
    for(int i=0;i<n;i++)
    {
        sum=0;
        cin>>p>>q;
        vector <int> arr(p);
        for(int j=0;j<p;j++)
        {
            cin>>arr[j];
            if (arr[j]<=0)
                sum++;
        }
        if(sum>=q)
            cout<<"NO"<<"\n";
        else
            cout<<"YES"<<"\n";
    }
}
